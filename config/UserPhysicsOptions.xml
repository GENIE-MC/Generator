  <param type="double" name="BY-Cv2D">  0.255   </param>
  <param type="double" name="BY-X0">   -0.00817 </param>
  <param type="double" name="BY-X1">    0.0506  </param>
  <param type="double" name="BY-X2">    0.0798  </param>

