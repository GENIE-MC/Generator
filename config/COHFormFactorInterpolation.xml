<?xml version="1.0" encoding="ISO-8859-1"?>

<!--

Configuration sets for the COHFormFactorInterpolation

Configurable Parameters:
.........................................................................................................................
Name                          Type     Optional   Comment                                                    Default
.........................................................................................................................
COH-DV-FormFactor             vec-alg  No         Vector of DeVries sub algo and nuclei specification        -
AllowExtrapolation            bool     yes        Allow map extrapolation at higher pdg than map maximum     false
-->



<alg_conf>

  <param_set name="Default">

    <param type="vec-alg" name="COH-DV-FormFactor" delim=";">
      genie::DeVriesFormFactor/12C   ;
      genie::DeVriesFormFactor/40Ar
    </param>

    <param type="bool" name="AllowExtrapolation"> false  </param>
  </param_set>


</alg_conf>
