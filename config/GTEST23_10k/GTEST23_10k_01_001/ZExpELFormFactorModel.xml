    <param type="vec-double" name="QEL-Z_AP" delim=";"> -1.456913420810;0.060429778301;2.264610004099;2.508797757303      </param>
    <param type="vec-double" name="QEL-Z_BP" delim=";"> -3.998063570818;0.679719977200;6.645074212547;2.201417861981       </param>
    <param type="vec-double" name="QEL-Z_AN" delim=";"> 0.137711882882;-0.088362197564;0.807527807108;2.029827552515     </param>
    <param type="vec-double" name="QEL-Z_BN" delim=";"> 2.930923339199;0.560057274373;-0.396813399834;8.472227204543       </param>
    <!-- more factors can be added, if necessary according to Kmax -->

  </param_set>
</alg_conf>

